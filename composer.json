{
    "name": "b13/de-slash",
    "type": "typo3-cms-extension",
    "description": "In case you do not want to havea trailing slash in your URLs, this extension will remove it",
	"keywords": ["seo", "typo3", "trailing slash"],
	"license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^11.5 || ^12.4 || ^13.4"
    },
    "suggest": {
        "typo3/cms-seo": "*"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "de_slash",
            "web-dir": ".Build/Web",
            "app-dir": ".Build"
        }
    },
    "autoload": {
        "psr-4": {
            "B13\\DeSlash\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "B13\\DeSlash\\Tests\\": "Tests/"
        }
    },
    "require-dev": {
        "typo3/testing-framework": "^7.0 || ^8.0 || ^9.0",
        "saschaegerer/phpstan-typo3": "^1.8",
        "typo3/coding-standards": "^0.7",
        "typo3/cms-seo": "*"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin",
        "allow-plugins": {
            "typo3/cms-composer-installers": true,
            "typo3/class-alias-loader": true
        }
    }
}
